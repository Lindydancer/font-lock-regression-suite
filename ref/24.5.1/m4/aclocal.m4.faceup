# gives unlimited permission to copy and/or distribute it,
# with or without modifications, as long as this notice is preserved.
»
«x:# serial 2
»
«x:# _AM_PROG_TAR(FORMAT)
# --------------------
# Check how to create a tarball in format FORMAT.
# FORMAT should be one of `v7', `ustar', or `pax'.
#
# Substitute a variable $(am__tar) that is a command
# writing to stdout a FORMAT-tarball containing the directory
# $tardir.
#     tardir=directory && $(am__tar) > result.tar
#
# Substitute a variable $(am__untar) that extract such
# a tarball read from stdin.
#     $(am__untar) < result.tar
»AC_DEFUN([_AM_PROG_TAR],
[«x:# Always define AMTAR for backward compatibility.
»AM_MISSING_PROG([AMTAR], [tar])
m4_if([«v:$1»], [v7],
     [am__tar='${AMTAR} chof - "$$tardir"'; am__untar='${AMTAR} xf -'],
     [m4_case([«v:$1»], [ustar],, [pax],,
              [m4_fatal([Unknown tar «k:format»])])
AC_MSG_CHECKING([how to create a «v:$1» tar archive])
«x:# Loop over all known methods to create a tar archive until one works.
»_am_tools='gnutar m4_if([«v:$1»], [ustar], [plaintar]) pax cpio none'
_am_tools=${am_cv_prog_tar_«v:$1»-$_am_tools}
«x:# Do not fold the above two line into one, because Tru64 sh and
# Solaris sh will not grok spaces in the rhs of `-'.
»for _am_tool in $_am_tools
do
  case $_am_tool in
  gnutar)
    for _am_tar in tar gnutar gtar;
    do
      AM_RUN_LOG([$_am_tar --version]) && break
    done
    am__tar="$_am_tar --«k:format»=m4_if([«v:$1»], [pax], [posix], [«v:$1»]) -chf - "'"$$tardir"'
    am__tar_="$_am_tar --«k:format»=m4_if([«v:$1»], [pax], [posix], [«v:$1»]) -chf - "'"$tardir"'
    am__untar="$_am_tar -xf -"
    ;;
  plaintar)
    «x:# Must skip GNU tar: if it does not support --format= it doesn't create
»    «x:# ustar tarball either.
»    (tar --version) >/dev/null 2>&1 && continue
    am__tar='tar chf - "$$tardir"'
    am__tar_='tar chf - "$tardir"'
    am__untar='tar xf -'
    ;;
  pax)
    am__tar='pax -L -x «v:$1» -w "$$tardir"'
    am__tar_='pax -L -x «v:$1» -w "$tardir"'
    am__untar='pax -r'
    ;;
  cpio)
    am__tar='find "$$tardir" -print | cpio -o -H «v:$1» -L'
    am__tar_='find "$tardir" -print | cpio -o -H «v:$1» -L'
    am__untar='cpio -i -H «v:$1» -d'
    ;;
  none)
    am__tar=false
    am__tar_=false
    am__untar=false
    ;;
  esac

  «x:# If the value was cached, stop now.  We just wanted to have am__tar
»  «x:# and am__untar set.
»  test -n "${am_cv_prog_tar_«v:$1»}" && break

  «x:# tar/untar a dummy directory, and stop if the command works
»  rm -rf conftest.dir
  mkdir conftest.dir
  echo GrepMe > conftest.dir/«k:file»
  AM_RUN_LOG([tardir=conftest.dir && «k:eval» $am__tar_ >conftest.tar])
  rm -rf conftest.dir
  if test -s conftest.tar; then
    AM_RUN_LOG([$am__untar <conftest.tar])
    grep GrepMe conftest.dir/«k:file» >/dev/null 2>&1 && break
  fi
done
rm -rf conftest.dir

AC_CACHE_VAL([am_cv_prog_tar_«v:$1»], [am_cv_prog_tar_«v:$1»=$_am_tool])
AC_MSG_RESULT([$am_cv_prog_tar_«v:$1»])])
AC_SUBST([am__tar])
AC_SUBST([am__untar])
]) «x:# _AM_PROG_TAR
»
