«x:# AM_MAKE_INCLUDE()
# -----------------
# Check to see how make treats includes.
»AC_DEFUN([AM_MAKE_INCLUDE],
[am_make=${MAKE-make}
cat > confinc << 'END'
am__doit:
	@echo done
.PHONY: am__doit
END
«x:# If we don't find an include directive, just comment out the code.
»AC_MSG_CHECKING([for style of «k:include» used by $am_make])
am__«k:include»="«x:#"
»am__quote=
_am_result=none
«x:# First try GNU make style include.
»echo "include confinc" > confmf
«x:# We grep out `Entering directory' and `Leaving directory'
# messages which can occur if `w' ends up in MAKEFLAGS.
# In particular we don't look at `^make:' because GNU make might
# be invoked under some other name (usually "gmake"), in which
# case it prints its new name instead of `make'.
»if test "`$am_make -s -f confmf 2> /dev/null | grep -v 'ing directory'`" = "done"; then
   am__«k:include»=«k:include»
   am__quote=
   _am_result=GNU
fi
«x:# Now try BSD make style include.
»if test "$am__include" = "«x:#"; then
»   echo '.«k:include» "confinc"' > confmf
   if test "`$am_make -s -f confmf 2> /dev/null`" = "done"; then
      am__«k:include»=.«k:include»
      am__quote="\""
      _am_result=BSD
   fi
fi
AC_SUBST([am__«k:include»])
AC_SUBST([am__quote])
AC_MSG_RESULT([$_am_result])
rm -f confinc confmf
])

