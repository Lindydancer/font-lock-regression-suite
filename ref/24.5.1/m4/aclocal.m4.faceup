«x:# Prepare PATH_SEPARATOR.
# The user is always right.
»if test "${PATH_SEPARATOR+set}" != set; then
  echo "«x:#! /bin/sh" >conf$$.sh
»  echo  "exit 0"   >>conf$$.sh
  chmod +x conf$$.sh
  if (PATH="/nonexistent;."; conf$$.sh) >/dev/null 2>&1; then
    PATH_SEPARATOR=';'
  else
    PATH_SEPARATOR=:
  fi
  rm -f conf$$.sh
fi

«x:# Find out how to test for executable files. Don't use a zero-byte file,
# as systems may use methods other than mode bits to determine executability.
»cat >conf$$.«k:file» <<_ASEOF
«x:#! /bin/sh
»exit 0
_ASEOF
chmod +x conf$$.«k:file»
if test -x conf$$.«k:file» >/dev/null 2>&1; then
  ac_executable_p="test -x"
else
  ac_executable_p="test -f"
fi
rm -f conf$$.«k:file»

«x:# Extract the first word of "$2", so it can be a program name with args.
»set dummy «v:$2»; ac_word=[$]2
AC_MSG_CHECKING([for $ac_word])
AC_CACHE_VAL(ac_cv_path_«v:$1»,
[case "[$]«v:$1»" in
  [[\\/]]* | ?:[[\\/]]*)
    ac_cv_path_«v:$1»="[$]«v:$1»" «x:# Let the user override the test with a path.
»    ;;
  *)
    ac_save_IFS="$IFS"; IFS=$PATH_SEPARATOR
    for ac_dir in «k:ifelse»([«v:$5»], , $PATH, [«v:$5»]); do
      IFS="$ac_save_IFS"
      test -z "$ac_dir" && ac_dir=.
      for ac_exec_ext in '' $ac_executable_extensions; do
        if $ac_executable_p "$ac_dir/$ac_word$ac_exec_ext"; then
          echo "$as_me: trying $ac_dir/$ac_word..." >&AS_MESSAGE_LOG_FD
          if [«v:$3»]; then
            ac_cv_path_«v:$1»="$ac_dir/$ac_word$ac_exec_ext"
            break 2
          fi
        fi
      done
    done
    IFS="$ac_save_IFS"
«x:dnl If no 4th arg is given, leave the cache variable unset,»
«x:dnl so AC_PATH_PROGS will keep looking.»
«k:ifelse»([«v:$4»], , , [  test -z "[$]ac_cv_path_«v:$1»" && ac_cv_path_«v:$1»="«v:$4»"
])«x:dnl»
    ;;
esac])«x:dnl»
«v:$1»="$ac_cv_path_«v:$1»"
if test «k:ifelse»([«v:$4»], , [-n "[$]«v:$1»"], ["[$]«v:$1»" != "«v:$4»"]); then
  AC_MSG_RESULT([$]«v:$1»)
else
  AC_MSG_RESULT(no)
fi
AC_SUBST(«v:$1»)«x:dnl»
])

